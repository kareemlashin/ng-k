{
  "name": "ng-k",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run sentry:sourcemaps",
    "watch": "ng build --watch --configuration development",
    "test:local": "ng test",
    "lint": "tslint -p tsconfig.json",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org kareem-p9 --project ng-k ./dist && sentry-cli sourcemaps upload --org kareem-p9 --project ng-k ./dist",

    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.html\"",
    "lint:fix": "tslint --fix -p tsconfig.json",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/ng-k/stats.json",
    "test:coverage": "ng test --watch=false --code-coverage",
    "e2e": "ng e2e",
    "clean": "rimraf dist .angular",
    "prepare": "husky install",
    "postinstall": "ngcc --properties es2015 browser module main"

  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@bugsnag/browser-performance": "^2.14.0",
    "@bugsnag/js": "^8.4.0",
    "@bugsnag/plugin-angular": "^8.4.0",
    "@sentry/angular": "^10.0.0",
    "@sentry/cli": "^2.50.2",
    "@sentry/tracing": "^7.120.4",
    "logrocket": "^10.1.0",
    "rollbar": "^2.26.4",
    "rxjs": "~7.8.0",
    "trackjs": "^3.10.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.7",
    "@angular/cli": "^19.1.7",
    "@angular/compiler-cli": "^19.1.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tslint": "^5.20.1",
    "typescript": "~5.7.2"
  }
}